    embed = [":go_default_library"],
    deps = [
        "//metricbeat/mb/testing:go_default_library",
        "//x-pack/metricbeat/module/mssql/testing:go_default_library",
        "@com_github_denisenkom_go_mssqldb//:go_default_library",
