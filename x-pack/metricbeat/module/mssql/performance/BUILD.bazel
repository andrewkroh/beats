    srcs = [
        "data.go",
        "performance.go",
    ],
    importpath = "github.com/elastic/beats/v7/x-pack/metricbeat/module/mssql/performance",
    visibility = ["//visibility:public"],
    deps = [
        "//libbeat/common:go_default_library",
        "//libbeat/common/schema:go_default_library",
